CubeCreate(vCube, pApprovalDim, vGroups, vDimRights, vDimRightsMeasures);
CubeSetLogChanges(vCube, 1);

CubeCreate(vCubeUpdates, pApprovalDim, vGroups, vDimRightsUpdateMeasures);
CubeSetLogChanges(vCubeUpdates, 1);


